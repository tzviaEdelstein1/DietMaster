{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "login-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [  
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/primeng/resources/themes/redmond/theme.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",    
        "styles.css",
        "generalCss.css"
      ],
      
      "scripts": ["../src/assets/js/agency.js",
        "../src/assets/js/agency.min.js",
"../src/assets/js/contact_me.js",
"../src/assets/js/contact_me.min.js",
"../src/assets/js/jqBootstrapValidation.js",
"../src/assets/js/jqBootstrapValidation.min.js",
"../src/assets/vendor/jquery/jquery.js",
"../src/assets/vendor/jquery/jquery.min.js",
"../src/assets/vendor/jquery/jquery.slim.js",
"../src/assets/vendor/jquery/jquery.slim.min.js",
"../src/assets/vendor/jquery/jquery.slim.min.map",
"../src/assets/vendor/jquery-easing/jquery.easing.compatibility.js",
"../src/assets/vendor/jquery-easing/jquery.easing.js",
"../src/assets/vendor/jquery-easing/jquery.easing.min.js"
      ],
        
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "spec": {
      "component": false
    },
    "styleExt": "css",
    "component": {}
  }
}